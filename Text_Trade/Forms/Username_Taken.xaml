<Window x:Class="Text_Trade.Username_Taken"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Text_Trade.Forms"
        mc:Ignorable="d"
        Title="Username_Taken" Height="200" Width="300">
    <Window.Background>
        <ImageBrush ImageSource="../Pictures/Text_Trade_background_big3.jpg" />
    </Window.Background>
    <Grid>
        <Button x:Name="okButton" Content="OK" HorizontalAlignment="Left" Margin="112,122,0,0" VerticalAlignment="Top" Width="75" Click="okButton_Click"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="56,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="199" Height="38"><Run Text="This username is unavailable."/><LineBreak/><Run Text="Please pick a different username."/></TextBlock>
        <Label x:Name="label" Content="Username Taken" HorizontalAlignment="Left" Margin="80,35,0,0" VerticalAlignment="Top" Width="134" FontSize="14" FontFamily="Stencil"/>

    </Grid>
</Window>
